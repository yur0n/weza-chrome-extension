{
	"manifest_version": 3,
	"name": "Weza",
	"version": "1.0",
	"description": "Extension",
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	},
	"permissions": [
		"background",
		"storage",
		"alarms"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_icon": {
      		"16": "icons/icon16.png",     
      		"24": "icons/icon16.png",     
      		"32": "icons/icon16.png"      
    	},
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"host_permissions": [
		"https://*/*",
		"http://*/*"
	]
}